<div class="order-browse">
    <div class="card">
        <div class="card-header clearfix">                                
            <ul class="order-list-type d-flex float-left">
                <li ng-if="SysPermission.showOrderTable()" class="list-item" ui-sref-active="active"><a ui-sref="orderlist.ordertable">订单列表</a></li>
                <li ng-if="SysPermission.showTimingTable()" class="list-item" ui-sref-active="active"><a ui-sref="orderlist.timingtable">定时列表</a></li>
                <li ng-if="SysPermission.showDownlinkTable()" class="list-item" ui-sref-active="active"><a ui-sref="orderlist.downlinktable">下行列表</a></li>
                <li ng-if="SysPermission.showUplinkTable()" class="list-item" ui-sref-active="active"><a ui-sref="orderlist.uplinktable">上行列表</a></li>
                <li ng-if="SysPermission.showVoicelinkTable()" class="list-item" ui-sref-active="active"><a ui-sref="orderlist.voicetable">语音会话列表</a></li>
            </ul>
            <div class="order-head-btn float-right d-flex">
            	<div class="num-text">
            		<b>{{per.OrederCount}}</b> 条记录
            	</div>
            	<div class="btn-cont">
            		<button type="button" ng-if="SysPermission.showExport()" class="btn btn-outline-info btn-sm" ng-click="importUserBalance()"> <i class="fa fa-cloud-download"></i> 导出</button>
            	</div>
            </div>
        </div>
        <div class="card-body order-body bg-fb">
			<ui-view></ui-view>
        </div>
    </div>
</div>