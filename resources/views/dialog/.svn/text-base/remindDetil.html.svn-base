<div class="modal-header justify-content-between">
    <h3 class="modal-title" id="modal-title">{{basedata.remindType}}</h3>
    <button type="button" class="btn btn-outline-close" ng-click="cancel()"><i class="fa fa-remove"></i></button>
</div>
<form class="form-inline">
    <div class="modal-body" id="modal-body">    	
      <div class="form-row">
		    <div class="form-group col-md-12" style="margin-bottom: 0.5rem;">
		      	<label class="col-md-2 control-label">内容</label>
		      	<textarea class="form-control col-md-10"  ng-model="basedata.msgContent" rows="3"></textarea>
		    </div>
		    <div class="form-group col-md-12" style="margin-bottom: 0.5rem;">
		      	<label class="col-md-2 control-label">类型</label>
		      	<input type="text" class="form-control col-md-10" ng-model="basedata.remindType" disabled placeholder="请输入">
		    </div>
			</div>
			<div class="form-row">
		    <div class="form-group col-md-12" style="margin-bottom: 0.5rem;">
		      	<label class="col-md-2 control-label">创建人</label>
		      	<input type="text" class="form-control col-md-10" ng-model="basedata.createUserName" disabled placeholder="">
		    </div>
		    <div class="form-group col-md-12" style="margin-bottom: 0.5rem;">
		      	<label class="col-md-2 control-label">创建时间</label>
		      	<input type="text" class="form-control col-md-10" ng-model="basedata.createTime" disabled placeholder="请输入">
		    </div>
			</div>
    </div>
    <div class="modal-footer justify-content-center" style="width: 100%;">
        <button class="btn btn-sm btn-light btn-light-border" type="button" ng-click="cancel()">关闭</button>
    </div>
</form>